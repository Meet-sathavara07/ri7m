{
  "expo": {
    "name": "RI7M",
    "slug": "ri7m",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./src//assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "versionCode": 2,
      "supportsTablet": true,
      "bundleIdentifier": "com.meetsathavara.ri7m"
    },
    "android": {
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.meetsathavara.ri7m"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      "expo-font",
      "expo-localization",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "0ee02f8f-01b6-4dd1-bdc5-37608d763797"
      }
    },
    "owner": "meetsathavara",
    "updates": {
      "url": "https://u.expo.dev/0ee02f8f-01b6-4dd1-bdc5-37608d763797"
    },
    "runtimeVersion": "2.0.0"
  }
}
